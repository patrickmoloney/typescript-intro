{
  "extends": "tslint:recommended",
  "rulesDirectory": ["node_modules/codelyzer"],
  "rules": {
      "max-line-length": {
          "options": [250]
      },
      "new-parens": true,
      "no-arg": true,
      "no-bitwise": true,
      "no-conditional-assignment": true,
      "no-consecutive-blank-lines": false,
      "no-console": false,
      "ordered-imports": false,
      "trailing-comma": false
  },
  "jsRules": {
      "max-line-length": {
          "options": [250]
      }
  }
}